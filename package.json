{
  "name": "@mhbdev/mem-kit",
  "version": "0.1.2",
  "description": "Minimal, Pluggable LLM Memory SDK",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "build:tsc": "tsc -p tsconfig.json",
    "bench:build": "tsup src/benchmarks/CostBenchmarks.ts --format cjs --out-dir dist/benchmarks --clean --no-dts",
    "bench:cost": "npm run bench:build && node dist/benchmarks/CostBenchmarks.cjs",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "llm",
    "memory",
    "rag",
    "embeddings",
    "typescript",
    "sdk"
  ],
  "author": "MonoBit",
  "license": "MIT",
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.9",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@types/pg": "^8.15.6",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "optionalDependencies": {
    "better-sqlite3": "^9.3.0",
    "drizzle-orm": "^0.33.0",
    "pg": "^8.16.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.4.1"
}
